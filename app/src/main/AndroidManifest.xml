<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rkwguapp">


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <receiver
            android:name=".AlertReceiver"
            android:enabled="true"
            android:exported="true"></receiver>

        <activity android:name=".AddEditAssessment"
            android:parentActivityName=".MainAssessment"/>
        <activity android:name=".MainAssessment"
            android:launchMode="singleTop"/>

        <activity android:name=".AddEditCourses"
            android:parentActivityName=".MainCourse"/>
        <activity android:name=".MainCourse"
            android:launchMode="singleTop"/>

        <activity
            android:name=".AddEditTerm"
            android:parentActivityName=".MainTerm" />
        <activity
            android:name=".MainTerm"
            android:launchMode="singleTop" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>